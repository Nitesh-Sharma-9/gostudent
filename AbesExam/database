--
-- Definition of procedure `setMode`
--

DROP PROCEDURE IF EXISTS `setMode`;

DELIMITER $$

/*!50003 SET @TEMP_SQL_MODE=@@SQL_MODE, SQL_MODE='' */ $$
CREATE DEFINER=`root`@`localhost` PROCEDURE `setMode`(
   vregid varchar(20)
)
BEGIN
update tbllogin set wmode='Y' where regid=vregid;
END $$
/*!50003 SET SESSION SQL_MODE=@TEMP_SQL_MODE */  $$

DELIMITER ;

--
-- Definition of procedure `verify`
--

DROP PROCEDURE IF EXISTS `verify`;

DELIMITER $$

/*!50003 SET @TEMP_SQL_MODE=@@SQL_MODE, SQL_MODE='' */ $$
CREATE DEFINER=`root`@`localhost` PROCEDURE `verify`(
  vregid varchar(20),
  vscode int(8),
  out msg varchar(100)
)
BEGIN
declare continue handler for 1329 # if no data found
   set msg = 'Sorry this is invalid Login Id & Password, try again.';
 select wmode into msg from tbllogin where scode=vscode and regid=vregid;
END $$
/*!50003 SET SESSION SQL_MODE=@TEMP_SQL_MODE */  $$

DELIMITER ;



