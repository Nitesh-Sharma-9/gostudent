--
-- Definition of procedure `addPwd`
--

DROP PROCEDURE IF EXISTS `addPwd`;

DELIMITER $$

/*!50003 SET @TEMP_SQL_MODE=@@SQL_MODE, SQL_MODE='' */ $$
CREATE DEFINER=`root`@`localhost` PROCEDURE `addPwd`(
 vregid varchar(20),
 vpass varchar(15),
 vscode int(8),
 vmailid varchar(50),
 vmob varchar(15),
 out msg varchar(100)
)
BEGIN
declare continue handler for 1062 # if user already exist hai to
    set msg = 'Sorry user already exist, try another user Id';
 insert into tbllogin (regid,pass,scode,mailid,mob) values(vregid,vpass,vscode,vmailid,vmob);
set msg='Record inserted..........';
commit;
END $$
/*!50003 SET SESSION SQL_MODE=@TEMP_SQL_MODE */  $$

DELIMITER ;

--
-- Definition of procedure `addSec`
--

DROP PROCEDURE IF EXISTS `addSec`;

DELIMITER $$

/*!50003 SET @TEMP_SQL_MODE=@@SQL_MODE, SQL_MODE='' */ $$
CREATE DEFINER=`root`@`localhost` PROCEDURE `addSec`(
   vregid varchar(20),
   vques varchar(200),
   vans varchar(200),
   out msg varchar(100)
)
BEGIN
insert into tblSec values(vregid,vques,vans);
set msg='Record inserted.....';
commit;

END $$
/*!50003 SET SESSION SQL_MODE=@TEMP_SQL_MODE */  $$

DELIMITER ;

--
-- Definition of procedure `addUser`
--

DROP PROCEDURE IF EXISTS `addUser`;

DELIMITER $$

/*!50003 SET @TEMP_SQL_MODE=@@SQL_MODE, SQL_MODE='' */ $$
CREATE DEFINER=`root`@`localhost` PROCEDURE `addUser`(
  vregid varchar(20),
  vname varchar(30),
  vfname varchar(30),
  vmname varchar(30),
  vgender varchar(6),
  vmarks_10  decimal(3,2),
  vmarks_12  decimal(3,2),
  vmarks_ug  decimal(3,2),
  vmarks_pg  decimal(3,2),
  vquali_ug varchar(10),
  vquali_pg varchar(10),
  vstate    varchar(20),
  vcity     varchar(20),
  vcollege  varchar(20),
  vdob varchar(15),
  vmob varchar(15),
  vlandline varchar(15),
  vmailid varchar(50),
  vcurr_date varchar(30),
  vtemp_addr varchar(200),
  vperm_addr varchar(200),
  out msg varchar(100)
)
BEGIN
   declare continue handler for 1062 # if user already exist hai to
    set msg = 'Sorry user already exist, try another user Id';
   insert into  tblregis values(
   vregid,vname,vfname,vmname,vgender,vmarks_10,
   vmarks_12,vmarks_ug,vmarks_pg,vquali_ug,vquali_pg,
   vstate,vcity,vcollege,vdob,concat('+91',vmob),
   vlandline,vmailid,vcurr_date,vtemp_addr,vperm_addr
   );
   set msg='Record successfully inserted.....';
commit;
END $$
/*!50003 SET SESSION SQL_MODE=@TEMP_SQL_MODE */  $$

DELIMITER ;

--
-- Definition of procedure `checkId`
--

DROP PROCEDURE IF EXISTS `checkId`;

DELIMITER $$

/*!50003 SET @TEMP_SQL_MODE=@@SQL_MODE, SQL_MODE='' */ $$
CREATE DEFINER=`root`@`localhost` PROCEDURE `checkId`(
  vregid varchar(20),
  vpass varchar(15),
  out msg varchar(100)
)
BEGIN
 select wmode into msg from tbllogin where regid=vregid and pass=vpass;
END $$
/*!50003 SET SESSION SQL_MODE=@TEMP_SQL_MODE */  $$

DELIMITER ;

--
-- Definition of procedure `forget`
--

DROP PROCEDURE IF EXISTS `forget`;

DELIMITER $$

/*!50003 SET @TEMP_SQL_MODE=@@SQL_MODE, SQL_MODE='' */ $$
CREATE DEFINER=`root`@`localhost` PROCEDURE `forget`(
vregid varchar(20),
out msg varchar(100)
)
BEGIN
update tbllogin set pass=date_format(sysdate(),'%d%H%s%m') where regid=vregid;
commit;
select pass into msg from tbllogin where regid=vregid;
END $$
/*!50003 SET SESSION SQL_MODE=@TEMP_SQL_MODE */  $$

DELIMITER ;

--
-- Definition of procedure `getAllCity`
