--
-- Definition of table `tblregis`
--

DROP TABLE IF EXISTS `tblregis`;
CREATE TABLE `tblregis` (
  `regid` varchar(20) NOT NULL,
  `name` varchar(30) NOT NULL,
  `fname` varchar(30) NOT NULL,
  `mname` varchar(30) NOT NULL,
  `gender` varchar(6) NOT NULL,
  `marks_10` decimal(3,2) DEFAULT NULL,
  `marks_12` decimal(3,2) DEFAULT NULL,
  `marks_ug` decimal(3,2) DEFAULT NULL,
  `marks_pg` decimal(3,2) DEFAULT NULL,
  `quali_ug` varchar(10) DEFAULT NULL,
  `quali_pg` varchar(10) DEFAULT NULL,
  `state` varchar(20) NOT NULL,
  `city` varchar(20) DEFAULT NULL,
  `college` varchar(20) DEFAULT NULL,
  `dob` varchar(15) NOT NULL,
  `mob` varchar(15) NOT NULL,
  `landline` varchar(15) DEFAULT NULL,
  `mailid` varchar(50) NOT NULL,
  `curr_date` varchar(30) DEFAULT NULL,
  `temp_addr` varchar(200) DEFAULT NULL,
  `perm_addr` varchar(200) DEFAULT NULL,
  PRIMARY KEY (`regid`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1;

--
-- Dumping data for table `tblregis`
--

/*!40000 ALTER TABLE `tblregis` DISABLE KEYS */;
/*!40000 ALTER TABLE `tblregis` ENABLE KEYS */;


